<?xml version='1.0' encoding='utf-8'?><widget id="com.cesalv.santander2015" version="1.0.1" xmlns:gap = "http://phonegap.com/ns/1.0">    <name>Semana Grande 2015</name>    <description>        Programa de la Semana Grande Santander 2015    </description>    <author email="cesalv@gmail.com" href="http://cesalv.com">        Cesalv    </author>    <content src="index.html" />    <access origin="*" />    <preference name="Fullscreen" value="true" />    <preference name="WebViewBounce" value="true" />    <preference name="Orientation" value="portrait" />    <preference name="phonegap-version" value="3.7.0" />    <preference name="LoadingDialog" value="Cargando"/>    <preference name="BackgroundColor" value="0x000000"/>	<preference name="KeepRunning" value="false"/>	<preference name="exit-on-suspend" value="true" />	<gap:platform name="android" />	<gap:plugin name="org.apache.cordova.splashscreen"  />	<gap:splash src="splash.png" gap:platform="android" gap:density="ldpi" />	<gap:splash src="splash.png" gap:platform="android" gap:density="mdpi" />	<gap:splash src="splash.png" gap:platform="android" gap:density="hdpi" />	<gap:splash src="splash.png" gap:platform="android" gap:density="xhdpi" />     <icon src="icon.png" /></widget>